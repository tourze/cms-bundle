{
  "name": "tourze/cms-bundle",
  "description": "CMS管理",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/collections": "^2.3",
    "doctrine/dbal": "^4.0",
    "doctrine/persistence": "^3.1 || ^4",
    "doctrine/orm": "^3.0",
    "doctrine/doctrine-bundle": "^2.13",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/enum-extra": "~0.0.5",
    "tourze/easy-admin-attribute": "0.0.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/doctrine-track-bundle": "0.0.*",
    "tourze/doctrine-helper": "0.0.*",
    "tourze/easy-admin-extra-bundle": "0.0.*",
    "tourze/doctrine-snowflake-bundle": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "CmsBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CmsBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  }
}